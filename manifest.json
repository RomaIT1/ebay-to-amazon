{
  "action": {
    "default_icon": "/img/logo.png",
    
    "default_title": "Chrome Extansion"
  },
  "content_scripts": [
    {
      "all_frames": false,
      "js": ["/js/content.js"],
      "matches": ["https://*/*"],
      "run_at": "document_start"
    }
  ],
  "description": "Speechy uses AI based analysis to provide feedback and recommendations to passively improve your English speech.",
  "icons": {
    "128": "/img/logo.png",
    "512": "/img/logo.png"
  },
  "manifest_version": 3,
  "name": "Chrome Extension",
  "permissions": ["storage"],
  "host_permissions": [],
  "version": "1.0.0",
  "web_accessible_resources": [
    {
      "resources": ["*"],
      "matches": ["https://*/*"],
      "extension_ids": []
    }
  ]
}